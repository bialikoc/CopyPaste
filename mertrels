    <script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/js/all.min.js"></script>
    <script>
        var toggleButton = document.querySelector('.toggle-sidebar');
        var sidebarContent = document.querySelector('.sidebar-content');

        toggleButton.addEventListener('click', function() {
            sidebarContent.style.display = (sidebarContent.style.display === 'none') ? 'block' : 'none';
        });
        
          function hideSidebarContent() {
    var sidebarContent = document.querySelector('.sidebar-content');
    sidebarContent.style.display = 'none';
  }
    </script>
    


<style>
    /* Normal stil kuralları buraya gelecek */
            .sidebar-mobil {
            position: fixed;
            left: 0;
            top: 50%;
            width: 40px;
            height: 50px;
            color: #333;
            background-color: rgba(72, 136, 183, 0.8);
            text-align: center;
            cursor: pointer;
